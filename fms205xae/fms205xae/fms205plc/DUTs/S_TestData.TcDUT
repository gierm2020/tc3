<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.11">
  <DUT Name="S_TestData" Id="{e7d79184-8d20-4b7d-b4d7-fecafb10a6e3}">
    <Declaration><![CDATA[TYPE S_TestData :
STRUCT
	(* Estructura para guardar los datos que se necesitan para el modo de pruebas: Execute, Ack y módulos a ejecutar *)
	Execute		: BOOL; // Indica a los módulos que empiecen a hacer su trabajo
	Ack			: BOOL; // Indica a los módulos que se ha completado el ciclo y pueden volver al principio
	Negar		: BOOL; // Cambia los sensores de TRUE a FALSE o viceversa para facilitar las pruebas cuando no hay hardware
	FlancoNegar	: R_TRIG; // Cuando se pulsa Negar sólo se ejecuta una vez

	Alime		: BOOL; // Probar el alimentador
	Carga		: BOOL; // Probar el brazo de carga
	Medid		: BOOL; // Probar el medidor
	Fotoe		: BOOL; // Probar el detector  fotoeléctrico
	Induc		: BOOL; // Probar el captador inductivo
	Evacu		: BOOL; // Probar el evacuador
	Desca		: BOOL; // Probar el brazo de descarga
	Plato		: BOOL; // Plato giratorio
	Cinta		: BOOL; // Probar la cinta
END_STRUCT
END_TYPE
]]></Declaration>
  </DUT>
</TcPlcObject>